{
  "name": "sarah-bot",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "node ./build-env.js && sst start",
    "start-a": "node ./build-env.js && echo '\nadam=true' >> .env && sst start --stage alocal",
    "build": "node ./build-env.js && sst build",
    "deploy": "node ./build-env.js && sst deploy",
    "deploy-a": "node ./build-env.js && echo '\nadam=true' >> .env && sst deploy --stage alocal",
    "remove": "sst remove",
    "console": "sst console",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@serverless-stack/cli": "^1.8.2",
    "@serverless-stack/resources": "^1.8.2",
    "@tsconfig/node16": "^1.0.3",
    "@types/pdf-image": "^2.0.1",
    "@types/pdf-parse": "^1.1.1",
    "@types/pdfkit": "^0.12.9",
    "@types/shelljs": "^0.8.12",
    "aws-cdk-lib": "2.32.0",
    "typescript": "^4.7.4",
    "vitest": "^0.22.1"
  },
  "workspaces": [
    "services"
  ],
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "aws-sdk": "^2.1199.0",
    "axios": "^0.27.2",
    "dotenv": "^16.0.3",
    "jest": "^29.5.0",
    "node-cloud-vision-api": "^0.2.0",
    "openai": "^3.2.1",
    "pdf-merger-js": "^4.3.0",
    "pdf-parse": "^1.1.1"
  }
}
